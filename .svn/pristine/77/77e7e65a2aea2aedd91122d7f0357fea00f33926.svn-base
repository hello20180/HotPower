package com.hnzy.hot.xtgl.pojo;

import java.io.Serializable;
import java.util.Date;

public class Bsj implements Serializable
{
	private int id;
   private String cjr;
   private Date cjsj;
   private String cjNr;
   
public int getId()
{
	return id;
}
public void setId(int id)
{
	this.id = id;
}
public String getCjr()
{
	return cjr;
}
public void setCjr(String cjr)
{
	this.cjr = cjr;
}
public Date getCjsj()
{
	return cjsj;
}
public void setCjsj(Date cjsj)
{
	this.cjsj = cjsj;
}
public String getCjNr()
{
	return cjNr;
}
public void setCjNr(String cjNr)
{
	this.cjNr = cjNr;
}
   
}
