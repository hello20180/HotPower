package com.hnzy.hot.hrzgl.pojo;

import java.io.Serializable;
import java.util.Date;

public class HeatWX implements Serializable
{
	private int id;
	private String title;
	private String article;
	private String wx;
	private Date wxTime;
	private String remark;
	private String image;
	private String hesName;
	
	public String getHesName()
	{
		return hesName;
	}
	public void setHesName(String hesName)
	{
		this.hesName = hesName;
	}
	public int getId()
	{
		return id;
	}
	public void setId(int id)
	{
		this.id = id;
	}
	public String getTitle()
	{
		return title;
	}
	public void setTitle(String title)
	{
		this.title = title;
	}
	public String getArticle()
	{
		return article;
	}
	public void setArticle(String article)
	{
		this.article = article;
	}
	public String getWx()
	{
		return wx;
	}
	public void setWx(String wx)
	{
		this.wx = wx;
	}
	public Date getWxTime()
	{
		return wxTime;
	}
	public void setWxTime(Date wxTime)
	{
		this.wxTime = wxTime;
	}
	public String getRemark()
	{
		return remark;
	}
	public void setRemark(String remark)
	{
		this.remark = remark;
	}
	public String getImage()
	{
		return image;
	}
	public void setImage(String image)
	{
		this.image = image;
	}
	
	
	
}
